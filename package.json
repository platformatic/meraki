{
  "name": "meraki",
  "private": true,
  "version": "0.0.1",
  "main": "./out/main/index.js",
  "author": "Platformatic Inc.",
  "scripts": {
    "lint": "standard . | snazzy",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "test:main": "standard | snazzy && vitest ./test/main --no-threads",
    "test:renderer": "standard | snazzy && vitest ./test/renderer --no-threads",
    "test": "npm run test:main && npm run test:renderer",
    "clean": "rm -Rf ./out && rm -Rf ./dist && rm -Rf ./coverage",
    "release:win": "electron-builder -p always --win --config",
    "release:linux": "electron-builder -p always --linux --config",
    "release:mac": "electron-builder -p always --mac --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.1",
    "@fastify/error": "^3.4.1",
    "@platformatic/ui-components": "^0.1.126",
    "autoprefixer": "^10.4.16",
    "boring-name-generator": "^1.0.3",
    "electron-updater": "^6.1.7",
    "execa": "^5.1.1",
    "mkdirp": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "tailwindcss": "^3.3.5",
    "undici": "^5.28.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.0.4",
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1",
    "electron-vite": "^1.0.29",
    "fastify": "^4.24.3",
    "glob": "^10.3.10",
    "happy-dom": "^12.10.3",
    "react": "^18.2.0",
    "snazzy": "^9.0.0",
    "standard": "^17.1.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  }
}
